{
    "configurations": [
        {
            "type": "java",
            "name": "Launch Passgenerator",
            "request": "launch",
            "mainClass": "com.security.security.util.Passgenerator",
            "projectName": "security"
        },
        {
            "type": "java",
            "name": "Spring Boot-SecurityApplication<security>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
